<h1>Songs#edit</h1>
<p>Find me in app/views/songs/edit.html.erb</p>

<h1>Song Form: </h1>
<%= form_for @song do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :artist_id %><br>
  <%= f.number_field :artist_id %><br>

  <%= f.label :genre_id %><br>
  <%= f.number_field :genre_id %><br>

  <%= f.submit "Update Song" %>
<% end %>
