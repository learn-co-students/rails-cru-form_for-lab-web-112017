<h1>Create Song Form</h1>

<%= form_for(@song) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, placeholder: 'Name' %>
  <%= f.label :artist_id %>
  <%= f.text_field :artist_id, placeholder: 'Artist ID' %>
  <%= f.label :genre_id %>
  <%= f.text_field :genre_id, placeholder: 'Genre ID' %>
  <%= f.submit %>
<% end %>
